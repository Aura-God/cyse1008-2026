  "author": "Coulter",
  "version": "1.0.1",
  "description": "Next & JavaScript",
  "private": true,
  "scripts": {
    "dev:web": "dotenv -e .env.development -- next dev -p 3032",
    "dev:emulators": "firebase emulators:start --import ./.firebase-data --export-on-exit",
    "dev": "npm-run-all -p dev:emulators dev:web",
    "start": "dotenv -e .env.black-river-market -- next start -p 3032",
    "build:base": "cross-env NEXT_PUBLIC_SKIP_ESLINT=true NEXT_PUBLIC_DEBUG=true npx next build --webpack",
    "build": "npm run build:base",
    "build:staging": "dotenv -e .env.black-river-market-staging -- npm run build:base",
    "build:prod": "dotenv -e .env.black-river-market -- npm run build:base",
    "start:staging": "dotenv -e .env.black-river-market-staging -- next start -p 3032",
    "start:prod": "dotenv -e .env.black-river-market -- next start -p 3032",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "start:out": "npx serve@latest out -p 3032"
  },
  "engines": {
    "node": "20"
  },
  "browser": {
    "fs": false,
    "os": false,
